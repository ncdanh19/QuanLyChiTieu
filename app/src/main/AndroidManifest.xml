<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sinhvien.quanlychitieu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_chitieu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_hanmuc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".activity.HangMucChiActivity"
            android:launchMode="singleTask"
            android:parentActivityName=".activity.TongQuanActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.TongQuanActivity"/>
        </activity>
        <activity android:name=".activity.TaoHanMucActivity" />
        <activity android:name=".activity.HanMucChiActivity" />
        <activity android:name=".activity.CustomTaiKhoan" />
        <activity android:name=".activity.CustomThuChi" />
        <activity android:name=".activity.ThongKeActivity" />
        <activity android:name=".activity.LoaiTaiKhoanActivity" />
        <activity android:name=".activity.TaoTaiKhoanActivity" />
        <activity
            android:name=".activity.TaiKhoanActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.HangMucActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.TongQuanActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ThuChiActivity" />
    </application>

</manifest>